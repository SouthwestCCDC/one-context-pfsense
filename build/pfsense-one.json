{
     "variables": {
       "iso": "pfsense.iso"
     },
     "builders": [
          {
               "type": "virtualbox-iso",
               "headless": true,
               "boot_wait": "7s",
               "boot_command": [
               "<wait>1",
               "<wait5><wait5><wait><wait>I",
               "<wait5><tab><tab><tab><wait><enter>",
               "<wait5><enter>",
               "<wait5><enter>",
               "<wait5><wait5><wait5><enter>",
               "<wait5><wait><enter>",
               "<wait5><wait5><wait5><wait5><wait5><wait5><wait5><wait5><wait5><wait5>",
               "<wait>n<enter>",
               "<wait>em0<enter>",
               "<wait>em1<enter>",
               "<wait><enter>",
               "<wait>y<enter>",
               "<wait5><wait5><wait5>",
               "8<enter><wait>",
               "cp /conf/config.xml /conf/config.xml.org<wait><enter>",
               "<wait5>reboot<enter>"
               ],
               "disk_size": 4096,
               "guest_additions_mode": "disable",
               "vm_name": "swccdc_pfsense_packer",
               "guest_os_type": "FreeBSD_64",
               "iso_url": "{{ user `iso` }}",
               "iso_checksum": "none",
               "shutdown_command": "8<enter>reboot<enter>",
               "vboxmanage": [
                 ["modifyvm", "{{.Name}}", "--memory", "2048"],
                 ["modifyvm", "{{.Name}}", "--cpus", "2"],
                 ["modifyvm", "{{.Name}}", "--audio", "none"],
                 ["modifyvm", "{{.Name}}", "--usb", "on"],
                 ["modifyvm", "{{.Name}}", "--clipboard-mode", "bidirectional"],
                 ["modifyvm", "{{.Name}}", "--mouse", "usbtablet"],
                 ["modifyvm", "{{.Name}}", "--graphicscontroller", "vmsvga"],
                 ["modifyvm", "{{.Name}}", "--vram", "16"]
               ],
               
               "ssh_username": "root",
               "ssh_password": "swccdc"
          }
     ],
     "provisioners": [
     ]
}
